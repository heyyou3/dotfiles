! CapsLock
keycode 66 = Control_L
add control = Control_L
! Swap Modifier Key
remove mod1 = Super_L
remove mod4 = Alt_L
keycode 64 = Super_L
keycode 133 = Alt_L
add mod1 = Alt_L

! Colemak Programmer
keycode 108 = Mode_switch
! right
keycode 38 = a A bracketleft
keycode 39 = r R braceleft
keycode 40 = s S less
keycode 41 = t T parenleft
keycode 42 = d D equal
keycode 43 = h H asterisk
keycode 44 = n N parenright
keycode 45 = e E greater
keycode 46 = i I braceright
keycode 47 = o O bracketright
keycode 48 = apostrophe quotedbl underscore
keycode 52 = z Z apostrophe
keycode 53 = x X quotedbl
keycode 54 = c C colon
keycode 55 = v V semicolon
keycode 56 = b B asciitilde
keycode 57 = k K grave
keycode 58 = m M backslash
! left
keycode 24 = q Q exclam
keycode 25 = w W at
keycode 26 = f F numbersign
keycode 27 = p P dollar
keycode 28 = g G percent
keycode 29 = j J asciicircum
keycode 30 = l L ampersand
keycode 31 = u U minus
keycode 32 = y Y plus
keycode 33 = semicolon colon bar

