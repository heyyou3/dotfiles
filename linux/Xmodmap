! CapsLock
keycode 66 = Control_L
add control = Control_L
! Swap Modifier Key
remove mod1 = Super_L
remove mod4 = Alt_L
keycode 64 = Super_L
keycode 133 = Alt_L
add mod1 = Alt_L

! Colemak Programmer
keycode 108 = Mode_switch
! right
keysym a = a A bracketleft
keysym r = r R braceleft
keysym s = s S less
keysym t = t T parenleft
keysym d = d D equal
keysym h = h H asterisk
keysym n = n N parenright
keysym e = e E greater
keysym i = i I braceright
keysym o = o O bracketright
keysym z = z Z underscore
keysym x = x X minus
keysym c = c C plus
! left
keysym q = q Q exclam
keysym w = w W at
keysym f = f F numbersign
keysym p = p P dollar
keysym g = g G percent
keysym j = j J asciicircum
keysym l = l L ampersand
keysym u = u U asciitilde
keysym y = y Y grave
! Sands
keycode 255 = space
keycode 65 = Shift_L
