[core]
  autocrlf = input
  editor = code --wait
  excludesfile = ~/.gitignore_global
[commit]
[alias]
  st = status
  co = checkout
  br = branch
  cm = commit
  df = diff
  gr = grep
  ph = push origin HEAD
[merge]
  tool = vimdiff
[init]
  defaultBranch = main
