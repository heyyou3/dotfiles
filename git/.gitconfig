[include]
  path = ~/.gitconfig_os
[core]
  autocrlf = input
  excludesfile = ~/.gitignore_global
  quotepath = false
  editor = nvr
  pager = delta --side-by-side
[interactive]
  diffFilter = delta --color-only
[delta]
  navigate = true
  dark = true
  line-numbers = true
  syntax-theme = "Dracula"
[init]
  defaultBranch = main
