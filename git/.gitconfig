[user]
  name = heyyou
  email = hey3you3@gmail.com
[core]
  autocrlf = input
  editor = vim
  excludesfile = ~/.gitignore_global
  quotepath = false
[commit]
[alias]
  st = status
  co = checkout
  br = branch
  cm = commit
  df = diff
  gr = grep
  ph = push origin HEAD
  pl = pull
  mr = merge
  l  = log
  a  = add

[merge]
  tool = vimdiff
[init]
  defaultBranch = main
